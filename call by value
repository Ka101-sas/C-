using System;
public class f1{
    public void m1(int val){
        val=val+10;
        Console.WriteLine(val);
    }
    public static void Main(){
        int val=15;
        Console.WriteLine("Value before funcn:"+val);
        f1 o1=new f1();
        o1.m1(val);
        Console.WriteLine("Value before funcn:"+val);

    }
}
